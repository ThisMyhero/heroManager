{
	"nbminer":{
		"file":"nbminer.exe",
		"runfile":"nbSecedit.exe",
		"bannet":1,
		"cmds":{
			"log":" -log-file {#LOG}",
			"api":" -api 127.0.0.1:{#APIPORT}",
			"param":" --share-check 120",
			"agent":0
		},
		"api":{
			"host":"http://127.0.0.1:{#APIPORT}",
			"path":"/api/v1/status",
			"hashrate":".miner.total_hashrate_raw"
		},
		"cv":{
			"busId_format":"{#BUSID}",
			"devices":"/miner/devices",
			"busId":"pci_bus_id",
			"version":"version",
			"pool":"/stratum/url",
			"user":"/stratum/user",
			"crypto":"/stratum/algorithm",	
			"latency":"/stratum/latency",
			"accepted_shares":"/stratum/accepted_shares",
			"rejected_shares":"/stratum/rejected_shares",
			"invalid_shares":"/stratum/invalid_shares",
			"difficulty":"/stratum/difficulty",
			"convert":{
				"lhr":"lhr",
				"fan":"fan",
				"power":"power",
				"temp":"temperature",
				"memTemp":"memTemperature",
				"speed":"hashrate_raw",
				"core":"core_clock",
				"memory":"mem_clock",
				"accepted_shares":"accepted_shares",
				"rejected_shares":"rejected_shares",
				"invalid_shares":"invalid_shares"
			}
		}
	},
	"gminer":{
		"file":"gminer.exe",
		"runfile":"gmSecedit.exe",
		"bannet":1,
		"cmds":{
			"log":" --logfile {#LOG}",
			"api":" --api 127.0.0.1:{#APIPORT}",
			"param":"",
			"agent":1
		},
		"api":{
			"host":"http://127.0.0.1:{#APIPORT}",
			"path":"/stat",
			"hashrate":".devices[].speed"
		},
		"cv":{
			"busId_format":"0000:{#2}{#BUSIDHEX}:00.0",
			"devices":"devices",
			"busId":"bus_id",
			"version":"miner",
			"pool":"server",
			"user":"user",
			"crypto":"algorithm",	
			"latency":"",
			"accepted_shares":"total_accepted_shares",
			"rejected_shares":"total_rejected_shares",
			"invalid_shares":"total_invalid_shares",
			"pool_speed":"pool_speed",
			"convert":{
				"fan":"fan",
				"power":"power_usage",
				"temp":"temperature",
				"memTemp":"memory_temperature",
				"speed":"speed",
				"core":"core_clock",
				"memory":"memory_clock",
				"accepted_shares":"accepted_shares",
				"rejected_shares":"rejected_shares",
				"invalid_shares":"invalid_shares"
			}
		}
	},
	"t-rex":{
		"file":"t-rex.exe",
		"runfile":"trexSecedit.exe",
		"bannet":1,
		"cmds":{
			"log":" --log-path {#LOG}",
			"api":" --api-bind-http 127.0.0.1:{#APIPORT}",
			"param":" --no-strict-ssl",
			"agent":0
		},
		"api":{
			"host":"http://127.0.0.1:{#APIPORT}",
			"path":"/summary?last-stat-ts=1649916068",
			"hashrate":".hashrate"
		},
		"cv":{
			"busId_format":"{#BUSID}",
			"devices":"gpus",
			"busId":"pci_bus",
			"version":"version",
			"pool":"/active_pool/url",
			"user":"/active_pool/user",
			"crypto":"algorithm",	
			"latency":"/active_pool/ping",
			"accepted_shares":"accepted_shares",
			"rejected_shares":"rejected_shares",
			"invalid_shares":"invalid_shares",
			"difficulty":"/active_pool/difficulty",
			"convert":{
				"lhr":"lhr_lock_count",
				"fan":"fan_speed",
				"power":"power",
				"temp":"temperature",
				"memTemp":"memory_temperature",
				"speed":"hashrate",
				"core":"cclock",
				"memory":"mclock",
				"accepted_shares":"/shares/accepted_shares",
				"rejected_shares":"/shares/rejected_shares",
				"invalid_shares":"/shares/invalid_shares"
			}
		}
	}
}