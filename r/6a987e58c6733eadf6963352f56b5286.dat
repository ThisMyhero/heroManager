function getTime(num){
    let d=num/86400
    let h=num/60/60
    let m=num/60%60
    if(d>1){
        return d.toFixed(1)+_mx('d')
    }else if(h>1){
        return h.toFixed(1)+_mx('h')
    }else{
        return m.toFixed(1)+_mx('m')
    }
}

