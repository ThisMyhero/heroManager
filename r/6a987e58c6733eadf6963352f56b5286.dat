            if(h==1){
                return 1
            }else {
                return _mx('Normal')
            }
        case 'anyhowmining':
            if(h==1){
                return 3
            }else {
                return _mx('Part work')
            }
        case 'anyhow_idle':
            if(h==1){
                return 2
            }else {
                return _mx('Occupied')
            }
        case 'disable':
            if(h==1){
                return 4
            }else {
                return _mx('MNF')
            }
        case 'unable':
            if(h==1){
                return 6
            }else {
                return _mx('UAM')
            }
        case 'idle':
            if(h==1){
                return 5
            }else {
                return _mx('Idle')
            }
        case 'starting':
            if(h==1){
                return 7
            }else {
                return _mx('starting')
            }
        case 'abnormal':
            if(h==1){
                return 8
            }else {
                return _mx('Abnormal')
            }
        default :
          return type
    }
}
//鼠标右键事件
function bindMenu(){

    var menu_list = document.getElementById("r_menu");

    var $BIPanel = $(".search_item").contextmenu(function(ev) {
        $('.search_item').removeClass('is_bagx')
        var oEvent = ev || event;
        selectDom=oEvent.target
        $(selectDom).addClass('is_bagx')
        menu_list.style.display = "block";
        menu_list.style.left = oEvent.clientX + "px";
        menu_list.style.top = oEvent.clientY + "px";
        return false;
    });

    document.onclick = function() {
        selectDom=undefined
        $('.search_item').removeClass('is_bagx')
        menu_list.style.display = "none";
    }
}
