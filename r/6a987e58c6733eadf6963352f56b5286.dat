function delFliter(e) {
    let x=[]
    $.each(listFilter,function (i,it) {
        if(!(it.value==$(e).attr('data-search')&&it.type==$(e).attr('data-type'))){
            x.push(it)
        }
    })
    e.remove()
    listFilter=x
    filterList(metadata)
}
