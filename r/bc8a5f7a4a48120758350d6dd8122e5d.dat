<!doctype html>
<html lang="zh-CN" translate="no">
<head>
    <title>Email</title>
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta charset="UTF-8">
    <meta name="robots" content="noindex">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="./css/themes/light/global.css" id="css" rel="stylesheet" type="text/css">
    <link href="./css/themes/dark/global.css" rel="stylesheet" type="text/css">
    <link href="./css/themes/light/email.css" rel="stylesheet" type="text/css">
    <link href="./css/themes/dark/email.css" rel="stylesheet" type="text/css">
</head>
<body>
<noscript>
    <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KJ552JD" height="0" width="0"
            style="display:none;visibility:hidden"></iframe>
</noscript>
<header>

</header>
<div class="page">
    <div class="menu">
        <div id="main_menu">

        </div>
    </div>
    <div class="content config_templates">
        <div class="title_row" style="justify-content: left">
            <h1 class="myTabText " onclick="linkWinHref('/site-config-templates')" lang-data-ht="Setting">Setting</h1>
            <h1 class="myTabText myTabTextIs" onclick="linkWinHref('/email-templates')" lang-data-ht="E-mail" style="margin-left: 30px">E-mail</h1>
            <h1 class="myTabText" onclick="linkWinHref('/auto-templates')" style="margin-left: 30px" lang-data-ht="Auto">Auto</h1>
            <h1 class="myTabText" onclick="linkWinHref('/clocktune')" style="margin-left: 30px" lang-data-ht="Overclocking">Overclocking</h1>
        </div>
        <div class="filter_row">
            <div class="search_el">
                <input type="text" id="search_configs"/>
                <div class="icon search"></div>
            </div>
            <div class="buttons">
                <div data-tooltip="export（JSON）" lang-data-ti="Export (JSON)" onclick="exportProfiles('all')" class="button white">
                    <div class="icon export"></div>
                </div>
                <div data-tooltip="import（JSON）" lang-data-ti="Import (JSON)" class="button white">
                    <div class="icon import"></div>
                    <input type="file" id="import" name="import"/></div>
                <div class="button green" onclick="EditWin()">
                    <div class="icon plus"></div>
                    <span lang-data-ht="Add New E-mail">Add New E-mail</span>
                </div>
            </div>
        </div>
        <div class="box_table">
            <table id="mytable">
            </table>
        </div>
        <div class="popupbackground close_popup"></div>
        <div class="popup" id="eMailWin">
            <div class="close" onclick="EditWinColse()"></div>
            <div class="title"><span lang-data-ht="Email Setting">Email Setting</span><br>
                <small style="color:#9698a4;" lang-data-ht="Set email account information here so that the system can push alarm messages">Set email account information here so that the system can push alarm messages</small>
            </div>
            <div class="text">
                <div lang-data-ht="SMTP Server">SMTP Server</div>
                <input id="ETPsever" class="value" type="text" style="margin-top: 10px;width: 100%">
                <!--                    <div style="margin-top:15px">Port</div>-->
                <!--                    <input id="Eport" class="value" type="text" style="margin-top: 10px;width: 100%">-->
                <div style="margin-top:15px" lang-data-ht="Email Account">Email Account</div>
                <input id="Eaccount" class="value" type="text" style="margin-top: 10px;width: 100%">
                <div style="margin-top:15px" lang-data-ht="Email Password/Authorization code">Email Password/Authorization code</div>
                <input id="Epassword" class="value" type="text" style="margin-top: 10px;width: 100%">
                <small lang-data-ht="Send a test Email before saving.  Make sure email configuration is correct.">Send a test Email before saving. Make sure email configuration is correct.</small>
            </div>
            <div class="action_row">
                <div class="circle-loader" id="myLoading">
                    <div class="checkmark draw"></div>
                </div>
                <div class="message_suc" id="mySuc">Succeed</div>
                <div class="message_err" id="myError">Error</div>
                <div class="button blue" onclick="saveTestEmail()" lang-data-ht="Test E-mail">Test E-mail</div>
                <div class="outlined_button gray close_popup" onclick="EditWinColse()" lang-data-ht="Close">Close</div>
                <div class="button green" id="saveBtn" onclick="saveEmail()" lang-data-ht="Save">Save</div>
            </div>
        </div>
        <div class="popup" id="email_delete">
            <div class="close"></div>
            <div class="title"></div>
            <div class="text">
                <div class="icon warning"></div>
                <p lang-data-ht="Do you want to delete the email address?"></p></div>
            <div class="action_row">
                <div class="circle-loader">
                    <div class="checkmark draw"></div>
                </div>
                <div class="message_suc">Finished</div>
                <div class="message_err">Error</div>
                <div class="outlined_button gray close_popup" lang-data-ht="No, keep it">No, keep it</div>
                <div class="button red" onclick="delConfigTemp(this)" lang-data-ht="Yes, delete it">Yes, delete it</div>
            </div>
        </div>
        <input type="hidden" id="nonce" value=""/>
    </div>
    <script>
        var jsonProfiles = '';
    </script>
</div>

<script>
    var currency = "USD";
    var currencyValue = 1.000000;
    var temperature = "°C";
    var userPermission = "owner";
    var userPrimaryPermission = "";

    [
        "./js/jquery.js", "./js/gettext.js", "./js/perfect-scrollbar.js",'./js/language.js', "./js/templates/global.js", "./js/templates/email.js", "./js/FileSaver.min.js"
    ].forEach(function (src) {
        var script = document.createElement("script");
        script.src = src;
        script.async = false;
        document.head.appendChild(script);
    });
</script>
</body>
</html>
