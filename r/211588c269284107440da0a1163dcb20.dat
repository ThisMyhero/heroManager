//删除接口
function delSiteSub(id){
    $('.circle-loader').show()
    $.ajax({
        url:'/manager',
        type:'post',
        data:{
            delete:id
        },
        success:function (res) {
            if(res==1){
                $('.circle-loader').addClass('load-complete');
                $('.notification_row').addClass('finished');
                $('.checkmark').show();
                let x=setTimeout(function () {
                    $('.circle-loader').removeClass('load-complete');
