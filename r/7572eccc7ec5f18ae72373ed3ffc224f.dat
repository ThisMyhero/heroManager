function workersSort(type) {
    sort = type;
    var d = new Date;
    d.setTime(d.getTime() + 24 * 60 * 60 * 1000 * 15);
    document.cookie = "workersSort=" + type + ";path=/;expires=" + d.toGMTString();
    workersPage(page, search, tag, type);
}

