            if (checkedNum == 0) {
                $('#checked_rows').hide();
                $('th').removeClass('hidden');
                $('#actions_menu').hide();
                selectedWorkers = [];

            } else {
                // var index = selectedWorkers.indexOf($(this).closest('[data-name]').attr('data-name'));
                var index = selectedWorkers.indexOf($(this).closest('[data-id]').attr('data-id'));
                if (index > -1) {
                    selectedWorkers.splice(index, 1);
                }
