function openDropdown(dropdownType) {
    if (!$('#' + dropdownType).closest('.col_1_2').hasClass('disabled')) {
        if ($('#' + dropdownType).css('display') == 'none' || $('#' + dropdownType).css("visibility") == "hidden") {
            $('.clocktune_values').hide();
        }
        $('#' + dropdownType).toggle();
    }
}

