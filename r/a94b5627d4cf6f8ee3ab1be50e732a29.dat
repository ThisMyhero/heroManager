//获取参数
function getQueryP(type) {
  let str=window.location.href
  let a=str.split('#')[1].split('&')
  let obj={}
  $.each(a,function (i,t) {
      obj[t.split('=')[0]]=t.split('=')[1]
  })
  return obj[type]
}
