function setFormData() {
    $.getJSON('/overclockquery?id='+getQueryP('id'),function (res) {
        setDomState(res)
    })
}
//设置界面数据回填
function setDomState(data) {
    let obj = data
    let isAshow=true
    let isNshow=true
    if (obj[0].type.indexOf('amd')!=-1){
        isAshow=true
    }else {
        isAshow=false
    }

    if (obj[0].type.indexOf('nvidia')!=-1){
        isNshow=true
    }else {
        isNshow=false
