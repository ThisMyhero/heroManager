.content.stats .header .right .change_worker_menu .frame .status.offline .bullet{background:#e2e5ef;}
.content.stats .header .right .change_worker_menu .frame .status.online .bullet{background:#52cca5;}
.content.stats .header .right .change_worker_menu .frame .status.idle .bullet{background:#ffd66f;}
.content.stats .header .right .change_worker_menu.small .frame a{margin:0;}
.content.stats .header .right .change_worker_menu .frame::-webkit-scrollbar-track{background-color:#e4e7f1;border-radius:30px;}
.content.stats .header .right .change_worker_menu .frame::-webkit-scrollbar{width:5px;background-color:#e4e7f1;border-radius:30px;}
.content.stats .header .right .change_worker_menu .frame::-webkit-scrollbar-thumb{background-color:#575e71;border-radius:30px;}

.content.stats .container .small_button{color:#acaebf;font-size:14px;font-weight:300;padding:10px 0 10px 0;margin:0;text-decoration:none;cursor:pointer;}
.content.stats .container .small_button:hover{color:#85879b;}
.content.stats .container [data-tooltip]:before{white-space:nowrap;font-weight:600;}

.content.stats .activity_log{}
