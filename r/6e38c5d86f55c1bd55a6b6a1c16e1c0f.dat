.content.stats .header .right .button{padding:8px;margin:0 0 0 10px;border-radius:3px;}
.content.stats .header .right .button .icon{width:18px;height:18px;background:url("/images/sprite_icons_button.svg?v=2") no-repeat;background-size:54px 900px;margin:0;float:none;}
.content.stats .header .right .button .icon.edit{background-position:0 -414px;}
.content.stats .header .right .button .icon.change{background-position:0 -432px;}
.content.stats .header .right .button .icon.details{background-position:-18px -486px;}
.content.stats .header .right .change_worker_menu{display:none;position:absolute;right:0;top:44px;border-radius:3px;background:#fff;box-shadow:0px 5px 8px rgba(0,0,0,0.1);z-index:99;}
