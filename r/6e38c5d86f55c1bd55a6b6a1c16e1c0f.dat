.content.stats .activity_log .row .icon_box .icon.scheduler{background-position:0 -770px;}
.content.stats .activity_log .row .icon_box .icon.clocktune{background-position:0 -396px;}
.content.stats .activity_log .row .icon_box .icon.config{background-position:0 -286px;}
.content.stats .activity_log .row .icon_box .icon.client_switch{background-position:0 -814px;}
.content.stats .activity_log .row .icon_box .icon.reboot{background-position:-22px -814px;}
.content.stats .activity_log .row .icon_box .icon.restart{background-position:0 -836px;}
