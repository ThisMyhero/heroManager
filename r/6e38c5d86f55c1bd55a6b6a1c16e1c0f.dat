.content.stats .notEnoughData .unavailable .image.nonotifications{background-position:-1400px 0px;}
.content.stats .notEnoughData .unavailable .title{width:100%;font-weight:700;color:#46474f;font-size:20px;margin:0 0 5px 0;}
.content.stats .notEnoughData .unavailable p{width:100%;line-height:1.15;}
.content.stats .notEnoughData .unavailable p b{display:block;}

