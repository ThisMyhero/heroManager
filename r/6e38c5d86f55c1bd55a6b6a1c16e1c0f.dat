/* STATS - GLOBAL */

.content.stats .container .col_4_5{width:calc(100% - 330px);display:block;}
.content.stats .container .col_4_5.full_width{width:100%;display:block;}
.content.stats .container .col_1_5{width:300px;margin-left:30px;}

.content.stats .header .right .button[data-tooltip]:before{left:auto;right:0px;transform:none;width:auto;white-space:nowrap;}
.content.stats .header .right .button[data-tooltip]:after{left:auto;right:10px;}

.content.stats .box_graph{padding:20px 20px 0 20px;margin-bottom:30px;display:block;width:100%;}
.content.stats .data_row{display:flex;flex-direction:row;justify-content:space-between;align-items:stretch;margin:0 -15px 30px -15px;}
.content.stats .data_row .element{margin:0 15px;background:#fff;border-radius:3px;box-shadow:0px 2px 0 rgba(0,0,0,0.05);flex:1;max-width:30%;}
.content.stats .data_row .element .divider{margin-bottom:0;}
.content.stats .data_row .element .divider_mini{border-top:1px solid #e2e5ef;margin:10px 0 -5px 0;}
.content.stats .data_row .element small{color:#969dae;font-size:14px;font-weight:300;padding:20px 20px 3px 20px;display:block;}
.content.stats .data_row .element .value{color:#575e71;font-size:24px;font-weight:600;padding:0 20px 5px 20px;display:block;}
.content.stats .data_row .element .change{color:#969dae;font-weight:300;padding:0 20px 10px 20px;display:flex;align-items:flex-start;font-size:12px;min-height:34px;}
.content.stats .data_row .element .change .icon{flex:none;width:14px;height:14px;background:url("/images/sprite_icons_button.svg?v=2") no-repeat;background-size:54px 900px;}
.content.stats .data_row .element .change .icon.arrow_up{background-position:0px -36px;}
.content.stats .data_row .element .change .icon.arrow_down{background-position:-18px -36px;}
.content.stats .data_row .element .change .icon.arrow_none{display:none;}
.content.stats .data_row .element .change span{margin-top:-1px;}
.content.stats .data_row .element .change span.red{color:#ff5b56;font-weight:600;margin-right:5px;font-size:14px;}
.content.stats .data_row .element .change span.green{color:#52cca5;font-weight:600;margin-right:5px;font-size:14px;}
.content.stats .data_row .element .change span.gray{color:#969dae;font-weight:600;margin-right:5px;font-size:14px;}
.content.stats .data_row .element .top_5{padding:10px 20px 10px 20px;}
.content.stats .data_row .element .top_5 .row{display:flex;margin:0;padding:10px 0 10px 0;border-bottom:1px solid #f8f8f8;flex-wrap:wrap;}
.content.stats .data_row .element .top_5 .row:last-child{border-bottom:0;}
.content.stats .data_row .element .top_5 .row .name{text-align:left;margin:0 auto 0 0;font-size:14px;color:#46474f;text-decoration:none;}
.content.stats .data_row .element .top_5 .row .name:hover{color:#52cca5;}
.content.stats .data_row .element .top_5 .row .value{text-align:right;margin:0 0 0 auto;padding:0;font-size:14px;display:flex;}
.content.stats .data_row .element .top_5 .row .value .progress_bar{margin:5px 0 0 0;height:4px;width:25px;border-radius:4px;background:#e2e5ef;overflow:hidden;}
.content.stats .data_row .element .top_5 .row .value .progress_bar .progress{height:4px;margin:0 auto 0 0;background:#52cca5;}
.content.stats .data_row .element .top_5 .row .value .progress_bar .progress.red{background:#ff5b56;}
.content.stats .data_row .element .top_5 .row .value .num{max-width:38px;min-width:38px;width:auto;}
.content.stats .data_row .element .top_5 .row .usd{width:auto;font-size:12px;margin:1px 0 0 21px;text-align:right;display:flex;align-items:center;color:#969dae;}
.content.stats .data_row .element .top_5 .row .coin{width:100%;font-size:14px;font-weight:600;margin:0 auto 0 0;text-align:left;display:flex;align-items:center;}
.content.stats .data_row .element .top_5 .row .coin[data-tooltip]:before{left:-10px;transform:none;}
.content.stats .data_row .element .top_5 .row .coin[data-tooltip]:after{left:5px;transform:none;}

.content.stats .data_row .element.addon{max-width:100%;flex:2 1 auto;background:#fff;align-items:center;padding:0;position:relative;min-height:270px;}
.content.stats .data_row .element.notEnoughData{max-width:100%;flex:2 1 auto;background:#fff;align-items:center;padding:0;position:relative;min-height:270px;display:none;border:0;}
.content.stats .data_row .element .dashed{border:2px dashed #e2e5ef;padding:15px 30px;position:absolute;top:6px;left:6px;right:6px;bottom:6px;display:flex;align-items:center;}
.content.stats .data_row .element .unavailable{width:100%;text-align:center;}
.content.stats .data_row .element .unavailable .image{width:100px;height:100px;background: url("/images/sprite_empty.svg") no-repeat;background-size:3000px 100px;margin:0 auto;}
.content.stats .data_row .element .unavailable .image.addon{background-position:-1200px 0px;display:table;}
.content.stats .data_row .element .unavailable .image.nodata{background-position:-1300px 0px;}
.content.stats .data_row .element .unavailable .title{width:100%;font-weight:700;color:#46474f;font-size:20px;margin:0 0 5px 0;}
.content.stats .data_row .element .unavailable p{width:100%;line-height:1.3;}
.content.stats .data_row .element .unavailable .button.blue{}

.content.stats h2{color:#575e71;font-size:18px;font-weight:300;}
.content.stats .distribution{margin:0 0 50px 0;}
.content.stats .distribution .row{display:flex;flex-direction:row;justify-content:flex-start;align-items:center;}
.content.stats .distribution .row .name{color:#969dae;width:90px;border-right:1px solid #e2e5ef;padding:10px 15px 10px 0;align-items:center;display:flex;position:relative;font-weight:300;font-size:16px;}
.content.stats .distribution .row .name[data-tooltip]:before{bottom:95%;left:-10px;transform:none;}
.content.stats .distribution .row .name[data-tooltip]:after{bottom:95%;left:5px;transform:none;}
.content.stats .distribution .row .progress_bar{padding-right:0;width:calc(100% - 90px);display:flex;}
.content.stats .distribution .row .progress_bar .progress{background:#52cca5;border-radius:0 3px 3px 0;height:12px;margin:1px 0 0 0;}
.content.stats .distribution .row .progress_bar .progress.red{background:#ff5b56;}
.content.stats .distribution .row .progress_bar .value{font-weight:600;font-size:16px;margin:0 0 0 20px;}

.content.stats .activity{margin:-10px 0 50px 0;}
.content.stats .activity .row{border-bottom:1px solid #e2e5ef;padding:10px 0;}
.content.stats .activity .row .data{display:flex;flex-direction:row;justify-content:flex-start;align-items:center;}
.content.stats .activity .row .data .value{color:#575e71;font-size:48px;font-weight:600;}
.content.stats .activity .row .data .name{color:#969dae;margin:0 0 0 10px;font-size:14px;line-height:1.2;}
.content.stats .activity .row .graph{display:flex;flex-direction:row;justify-content:space-between;align-items:center;}
.content.stats .activity .row .graph .progress_bar{margin:2px 0 0 0;height:4px;width:calc(100% - 50px);height:4px;border-radius:4px;background:#e2e5ef;overflow:hidden;}
.content.stats .activity .row .graph .progress_bar .progress{background:#52cca5;height:4px;margin:0;}
.content.stats .activity .row .graph .progress_bar .progress.red{background:#ff5b56;}
.content.stats .activity .row .graph .value{width:30px;font-weight:600;}

.content.stats ul.list{list-style:none;margin:20px 0 50px 0;}
.content.stats ul.list li{color:#575e71;font-size:14px;margin-bottom:10px;}
.content.stats ul.list li b{background:#fff;color:#575e71;font-size:16px;width:25px;background:#fff;border-radius:3px;box-shadow:0px 2px 0 rgba(0,0,0,0.05);font-weight:600;text-align:center;padding:5px;display:inline-block;margin:0 5px 0 0;}

/* STATS - PER WORKER */

.content.stats .header{display:flex;flex-direction:row;justify-content:space-between;padding-bottom:20px;}
.content.stats .header .left{display:flex;flex-direction:row;justify-content:flex-start;align-items:center;font-size:28px;color:#acaebf;font-weight:200;}
