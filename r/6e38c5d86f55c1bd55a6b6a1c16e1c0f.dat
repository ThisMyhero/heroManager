.content.stats table.poolsContent tr td .icon{width:18px;height:18px;background:url("/images/sprite_icons_button.svg?v=2") no-repeat;background-position:0 0;background-size:54px 900px;margin-bottom:0;}
.content.stats table.poolsContent tr td .icon.details{float:left;margin:0 10px 0 0;background-position:0 -288px}
.content.stats table.poolsContent tr td .poolsWorkersList{display:none;position:absolute;left:10px;top:40px;background:#fff;box-shadow:0 10px 30px rgba(0, 0, 0, 0.2);border-radius:3px;z-index:1;}
.content.stats table.poolsContent tr td .poolsWorkersList .frame{width:auto;min-width:220px;height:auto;max-height:290px;margin:10px;padding:0;display:block;overflow-y:auto;}
.content.stats table.poolsContent tr td .poolsWorkersList .frame a{font-size:16px;font-weight:300;color:#46474f;text-decoration:none;padding:10px;margin:0 10px 0 0;display:block;border-radius:2px;}
.content.stats table.poolsContent tr td .poolsWorkersList .frame a:hover{background:#f8f9fc;}
.content.stats table.poolsContent tr td .poolsWorkersList .frame::-webkit-scrollbar-track{background-color:#e4e7f1;border-radius:30px;}
.content.stats table.poolsContent tr td .poolsWorkersList .frame::-webkit-scrollbar{width:5px;background-color:#e4e7f1;border-radius:30px;}
.content.stats table.poolsContent tr td .poolsWorkersList .frame::-webkit-scrollbar-thumb{background-color:#575e71;border-radius:30px;}
.content.stats table.poolsContent tr th:last-child{text-align:left;}
.content.stats table.poolsContent tr td:last-child{text-align:left;}

.content.stats table tr td .tooltip{display:none;position:absolute;bottom:15px;transform:translateY(calc(100% + 10px));left:10px;z-index:3;cursor:default;font-family:'Mukta Mahee','Source Sans Pro',sans-serif;font-size:14px;font-weight:600;padding:7px 10px 5px 10px;width:auto;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;background-color:rgba(80, 82, 103, 1);background-color:rgba(80, 82, 103, 0.95);color:#fff;text-align:left;font-size:14px;line-height: 1.2;white-space:nowrap;}
.content.stats table tr td .arrow{position:absolute;top:-5px;left:10px;width:0;border-bottom:5px solid rgba(80, 82, 103, 1);border-bottom:5px solid rgba(80, 82, 103, 0.95);border-right:5px solid transparent;border-left:5px solid transparent;content:" ";font-size:0;line-height:0;}
.content.stats table tr td:hover .tooltip{display:block}

@media (max-width: 1600px) {
    .content.stats h2{width:100%;margin-left:10px;}
    .content.stats .container{flex-wrap:wrap;}
    .content.stats .container .col_1_5{width:calc(100% + 30px);display:flex;flex-direction:row;flex-wrap:wrap;margin:0 -15px;}
    .content.stats .container .col_1_5 .element{flex:1;background:#fff;box-shadow:0px 2px 0 rgba(0,0,0,0.05);border-radius:3px;margin:0 15px;padding:30px;}
    .content.stats .distribution{margin-bottom:0;}
    .content.stats .activity{margin-bottom:0;}
    .content.stats .activity .row:last-child{border-bottom:0;}
    .content.stats .activity .row .data .value{font-size:38px;}
    .content.stats .activity .row .data .name{line-height:1;}
    .content.stats .container .col_4_5{width:100%;}
    .content.stats .activity_log{width:calc(100% - 20px);display:table;margin:0 10px 20px 10px;}
    .content.stats .activity_log .frame{width:100%;max-height:380px;display:flex;flex-wrap:wrap;flex-direction:row;justify-content:flex-start;}
    .content.stats .activity_log .day_title{width:100%;margin-top:10px;}
    .content.stats .activity_log .row{width:calc(25% - 15px);margin:0 15px 15px 0;background:#fff;box-shadow:0px 2px 0 rgba(0,0,0,0.05);border-radius:3px;padding:15px;z-index:-1;}
    .content.stats .activity_log .row:last-child{padding:15px;}
    .content.stats .activity_log .row .icon_box{box-shadow:none;background:transparent;}
    .content.stats .activity_log .row .icon{box-shadow:none;background:transparent;width:20px;height:20px;}
    .content.stats .activity_log .row time{margin-top:0;}

    .content.stats #balanceStats .container .col_1_5{width:100%;margin:30px 0 0 0;}
    .content.stats #balanceStats .container .col_1_5 h1{width:100%;}
    .content.stats #balanceStats .wallets_list{width:100%;}
    .content.stats #balanceStats .coins_list{width:100%;}
    .content.stats .wallets_list .row{justify-content:flex-start;}
    .content.stats .wallets_list .row .button{width:auto;}

    .content.stats table.groupsAll tr .rmv2{display:none;}
    .content.stats table.groupsAll tr .shw2{display:block;clear:both;}
    .content.stats table.groupsAll tr .shw2 .row{display:flex;justify-content:space-between;flex-wrap:wrap;margin:10px 0 0 0;padding-left:20px;}
    .content.stats table.groupsAll tr .shw2 .row .element{flex:1;margin-right:20px;font-size:14px;}
    .content.stats table.groupsAll tr .shw2 .row .element:nth-child(3){flex:2;}
    .content.stats table.groupsAll tr .shw2 .row .element:nth-child(4){flex:2;}
    .content.stats table.groupsAll tr .shw2 .row .element .title{font-size:12px;color:#46474f;font-weight:700;margin:0 0 5px 0;}
    .content.stats table.groupsAll tr td .mini_stats .number small{margin-top:3px;}
    .content.stats table.groupsAll tr td .mini_stats .progress_bar{width:50px;}
    .content.stats table.groupsAll tr td .mini_stats .temps .temp{min-width:45px;}
    .content.stats table.groupsAll tr td .mini_stats .temps .temp small{margin-top:2px;}
    .content.stats table.groupsAll tr td .coins img{width:14px;height:14px;margin:0 5px 0 0;}

}
