.content.stats .activity_log .frame::-webkit-scrollbar-thumb{background-color:#575e71;border-radius:30px;}

.content.stats .notEnoughData{border:2px dashed #e2e5ef;;margin-top:6px;margin:6px;background:#fff;box-shadow:0px 0px 0px 6px #fff, 0px 8px 2px rgba(0,0,0,0.05);padding:10px 0;align-items:center;border-radius:1px;}
.content.stats .notEnoughData .unavailable{width:100%;text-align:center;}
