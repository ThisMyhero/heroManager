.content.stats .activity_log .row p b{color:#46474f;font-size:16px;font-weight:600;}
.content.stats .activity_log .frame{width:100%;height:auto;max-height:800px;margin:0;padding:0;display:block;position:relative;}
.content.stats .activity_log .frame::-webkit-scrollbar-track{background-color:#e4e7f1;border-radius:30px;}
