.my_icon{
    flex:none;
    width:10px;
    height:14px;
    background:url("/images/sprite_icons_button.svg") no-repeat;
    background-size:54px 900px;
}
.my_icon_x{
    flex:none;
    width:14px;
    height:14px;
    background:url("/images/sprite_icons_button.svg") no-repeat;
    background-size:54px 900px;
    margin-right: 5px;
}
.my_icon_x.ocmx{
    background-position:-36px -541px;
    width: 18px;height: 18px;margin-top: 10px
}
.my_icon_x.ocmx:hover{
    background-position:-18px -541px;
    cursor: pointer;
}
.my_icon_x_ocp{
    flex:none;
    width:16px;
    height:16px;
    background:url("/images/sprite_icons_button_dark.svg") no-repeat;
    background-size:54px 900px;
    margin-right: 5px;
}
.my_icon_x_ocp.search{
    background-position:-2px -90px;
    position: absolute;
    top: 5px;
    right: 3px;
}

.my_icon_xps_qh{
    width:20px;
    height:20px;
    background:url("/image/tab.svg") no-repeat;
    background-size:20px 20px;
    cursor: pointer;
    margin-top: 5px;
    margin-left: 10px;
    margin-right: 10px;
}
.my_icon_xps_qh:hover{
    width:20px;
    height:20px;
    background:url("/image/tab_h.svg") no-repeat;
    background-size:20px 20px;
    cursor: pointer;
}
.my_icon_xps_qh.mcsx{
    background:url("/image/mxz.svg") no-repeat;
    background-size: 20px 20px;
}
.my_icon_xps_qh.mcsx:hover{
    background:url("/image/mxz_h.svg") no-repeat;
    background-size: 20px 20px;
}
.my_icon_qh{
    width: 15px;
    height: 15px;
    background: url("/image/qh.svg") center;
    background-size: 15px 15px;
}
.my_icon.arrow_up{
    background-position:0px -36px;
}
.my_icon.arrow_down{
    background-position:-18px -36px;
}
.my_icon.arrow_none{
    display:none;
}
.my_icon_x.arrow_px_none{
    display:none;
}
.my_icon_x.arrow_px_up{
    background-position:3px -398px;
}
.my_icon_x.arrow_px_down{
    background-position:-15px -398px;
}
.my_icon_x.arrow_px_edit{
    margin-left: 15px;
    background-position:-38px -200px;
    cursor: pointer;
}.my_icon_x.arrow_px_edit:hover{
    margin-left: 15px;
    background-position:-20px -200px;
    cursor: pointer;
}
