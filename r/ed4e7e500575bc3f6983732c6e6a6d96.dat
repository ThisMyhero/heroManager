.site_list_card_wk{
  width: 320px;
  background-color: #1c2538;
  height: 100%;
padding-bottom: 20px;
}
.site_list_card_wk #list_card{
    height: calc(100vh + 70px);
    overflow-y: auto;overflow-x:hidden;
    /*width: 150%;*/
    /*margin-left: -25%;*/
}
.site_list_card_wk #list_card::-webkit-scrollbar{
    display: none;
}
#select_site_win .text::-webkit-scrollbar{
    display: none;
}
.site_list_card_wk .ti_tx{
    height: 75px;
    line-height: 75px;
    text-align: center;
    font-size: 28px;
}
 .item_card{
    margin: 0 auto;
    margin-top: 12px;
    width: 280px;
    height: 120px;
    cursor: pointer;
    border: 1px solid #1c2538;
    border-radius: 3px;
}
 .item_card .title{
    background-color: #232d43;
    height: 40px;
    font-size: 16px;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    text-align: center;
    line-height: 40px;
}
 .item_card .info_card{
    background-color: #343d52;
    height: 78px;
    font-size: 14px;
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
    text-align: center;
    padding-left: 20px;
    padding-right: 20px;
}
 .item_card .info_card .info_card_t{
    display: flex;
    font-size: 14px;
    line-height: 50px;
    justify-content: space-between;
}
.item_card .info_card .info_card_b{
    display: flex;
    font-size: 14px;
    justify-content: space-between;
}
 .item_card .info_card .info_card_t .sl{
    text-align: left;
    color: #fff;
}
