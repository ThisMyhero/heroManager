@keyframes loader-spin {
    0% {
        transform: rotate(0deg)
    }
    100% {
        transform: rotate(360deg)
    }
}

@keyframes checkmark {
    0% {
        height: 0;
        width: 0;
        opacity: 1
    }
    20% {
        height: 0;
        width: 5px;
        opacity: 1
    }
    40% {
        height: 10px;
        width: 5px;
        opacity: 1
    }
    100% {
        height: 10px;
        width: 5px;
        opacity: 1
    }
}

@keyframes loader-animation {
    0% {
        left: -100%
    }
    49% {
        left: 100%
    }
    50% {
        left: 100%
    }
    100% {
        left: -100%
    }
}

