}

#login .register_box .box_right .form .button.green {
    font-family: mukta mahee, sans-serif;
    font-size: 16px;
    background: #52cca5;
    border-radius: 3px;
    will-change: transform;
    transition: all .2s ease;
    border: 2px solid #52cca5;
    cursor: pointer;
    color: #fff;
    outline: none;
    text-align: center;
    margin: 30px auto 0 0;
    display: block;
    font-weight: 600;
    height: 40px;
    width: 100%;
    padding: 0 20px;
    display: flex;
    justify-content: center;
    align-items: center;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px
}

#login .register_box .box_right .form .button.green:focus {
    box-shadow: none
}

#login .register_box .box_right .form .button.green:hover {
    background: #3ab58e;
    border: 2px solid #3ab58e;
    color: #fff
}

#login .register_box .box_right .form .button.green.disabled {
    background: #e2e5ef;
    border-color: #e2e5ef;
    color: #fff;
    text-decoration: none;
    cursor: default
}

#login .register_box .box_right .form .button.green.disabled:focus {
    background-color: #e2e5ef;
    color: #fff;
    border-color: #e2e5ef
}

#login .register_box .box_right .form .button.green.disabled:hover {
    background-color: #e2e5ef;
    color: #fff;
    border-color: #e2e5ef
}

#login .register_box .box_right .form .button.green.active {
