}

#login .register_box .box_left {
    order: 1;
    flex: none;
    width: 50%;
    background: #5767ff;
    border-radius: 5px 0 0 5px;
    color: #fff;
    position: relative
}

#login .register_box .box_left .intro_photo {
    background: url(/images/register_intro.jpg) center bottom;
    background-size: cover;
    filter: blur(0);
    animation: blur-out 1000ms linear;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 2;
    border-radius: 5px 0 0 5px
}

#login .register_box .box_left .frame {
    display: none
}

#login .register_box .box_left .frame.visible {
    display: block
}

#login .register_box .box_left .frame .frame_content {
    min-width: 300px;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    z-index: 3
}

#login .register_box .box_left .frame .frame_content.appear {
    animation: appear 500ms linear
}

#login .register_box .box_left .frame .frame_content .title {
    font-weight: 800;
    font-size: 24px;
    text-align: center;
    margin: 0 auto 3px
}

#login .register_box .box_left .frame .frame_content .subtitle {
    font-weight: 200;
    font-size: 18px;
    text-align: center;
    margin: 0 auto 40px
}

#login .register_box .box_left .frame .frame_content ul {
    display: table;
    width: auto;
    margin: 0 auto;
    text-align: left;
    padding: 0;
    list-style: none
}

#login .register_box .box_left .frame .frame_content ul li {
    display: block;
    margin: 0 0 20px 32px;
    font-size: 16px;
    font-weight: 300;
    position: relative
}

#login .register_box .box_left .frame .frame_content ul li:before {
    content: "";
    width: 22px;
    height: 22px;
    border-radius: 24px;
    background: #fff;
    position: absolute;
    left: -32px;
    top: -3px;
    z-index: 4
}

#login .register_box .box_left .frame .frame_content ul li:after {
    content: "";
    width: 7px;
    height: 3px;
    border-radius: 0;
    background: 0 0;
    border-left: 2px solid #5767ff;
    border-bottom: 2px solid #5767ff;
    position: absolute;
    z-index: 5;
    left: -25px;
    top: 5px;
    transform: rotate(-45deg)
}

#login .register_box .box_left .frame .frame_content ul li:last-child {
    margin-bottom: 0
}

#login .register_box .box_left .dots {
    position: absolute;
    left: 50%;
    bottom: 30px;
    transform: translateX(-50%);
    z-index: 3;
    display: flex
}

#login .register_box .box_left .dots .dot {
    width: 10px;
    height: 10px;
    background: 0 0;
    border: 2px solid #fff;
    border-radius: 10px;
    margin: 0 5px;
    cursor: pointer
}

#login .register_box .box_left .dots .dot.selected {
    background: #fff
}

@keyframes blur-out {
    0% {
        opacity: 0;
        filter: blue(5px)
    }
    30% {
        opacity: 0;
        filter: blue(5px)
    }
    100% {
        opacity: 1;
        filter: blur(0)
    }
}

@keyframes appear {
    0% {
        opacity: 0;
        margin-top: -20px
    }
    100% {
        opacity: 1;
        margin-top: 0
    }
}

