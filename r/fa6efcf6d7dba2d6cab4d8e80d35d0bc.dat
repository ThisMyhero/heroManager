.mytoggle {

    width: 35px;
    height: 10px;
    border-radius: 30px;
    background: #676b78;
    position: relative;
    cursor: pointer
}

.mytoggle .bullet {
    background: #cbced8;
    width: 18px;
    height: 18px;
    position: absolute;
    right: auto;
    left: 0;
    top: -4px;
    border-radius: 30px;
    transition: left .2s linear
}

.mytoggle .bullet.selected {
    background: #52cca5;
    left: calc(100% - 18px);
    right: auto
}

