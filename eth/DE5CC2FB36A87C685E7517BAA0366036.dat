{
	"cmp":[
		{"name":"Radeon","eth":"teamredminer"},
		{"name":"AMD","eth":"teamredminer"},
		{"name":"NVIDIA","eth":"gminer"}
	],
	"type":{
		"teamredminer":{"cmd":"teamredminer.exe -a ethash -o stratum+tcp://{#url} -u {#wallet} --eth_worker {#name} --log_file=\"{#log_file}\" -p {#pass} --api_listen=0.0.0.0:{#port}"},
		"gminer":{"cmd":"gminer.exe -a ethash -s {#url} -u {#wallet}.{#name} --logfile \"{#log_file}\" -p {#pass} --api 0.0.0.0:{#port} --proto stratum"}
	}
}



