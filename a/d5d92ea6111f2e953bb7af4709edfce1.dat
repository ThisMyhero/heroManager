.content.triggers .notification {
    background: #575e71;
    border-radius: 3px;
    padding: 15px 20px;
    color: #fff;
    font-weight: 300;
    text-align: left;
    line-height: 1.15;
    margin: 0 0 20px;
    display: flex;
    align-items: flex-start;
    animation: warning_pulse 2s ease-out infinite
}

.content.triggers .notification b {
    font-weight: 600;
    color: #fff
}

.content.triggers .notification p {
    font-weight: 300;
    color: rgba(255, 255, 255, .8);
    margin: 0
}

.content.triggers .notification a:link, .content.triggers .notification a:visited, .content.triggers .notification a:active {
    color: #fff;
    font-weight: 600;
    text-decoration: underline
}

.content.triggers .notification a:hover, .content.triggers .notification a:visited:hover {
    opacity: .6
}

.content.triggers .notification a.button.white:link, .content.triggers .notification a.button.white:visited, .content.triggers .notification a.button.white:active {
    color: #ff5b56;
    background: #fff;
    border: 2px solid #fff;
    font-weight: 800;
    text-decoration: none;
    display: inline-table;
    margin: 10px 10px 0 0;
    padding: 8px 18px
}

.content.triggers .notification a.button.white:hover, .content.triggers .notification a.button.white:visited:hover {
    opacity: .6
}

.content.triggers .notification a.button_outlined.white:link, .content.triggers .notification a.button_outlined.white:visited, .content.triggers .notification a.button_outlined.white:active {
    color: #fff;
    background: 0 0;
    border: 2px solid #fff;
    font-weight: 800;
    text-decoration: none;
    display: inline-table;
    padding: 8px 18px;
    border-radius: 5px;
    margin: 10px 0 0
}

.content.triggers .notification a.button_outlined.white:hover, .content.triggers .notification a.button_outlined.white:visited:hover {
    opacity: .6
}

.content.triggers .notification.red {
    background: #ff5b56
}

.content.triggers .notification.green {
    background: #52cca5
}

.content.triggers .notification.blue {
    background: #5767ff
}

.content.triggers .notification .icon {
    flex: none;
    width: 32px;
    height: 32px;
    background: url(/images/sprite_icons_big.svg?v=2) no-repeat;
    background-size: 96px 96px;
    margin: 0 20px 0 0;
    display: block
}

.content.triggers .notification .icon.warning {
    background-position: 0 -32px
}

.content.triggers .notification .text {
}

.content.triggers {
}

.content.triggers .flexi {
    display: flex;
    flex-direction: column
}

.content.triggers .flexi .order1 {
    order: 1
}

.content.triggers .flexi .order2 {
    order: 2
}

.content.triggers .box {
    padding: 5px 10px;
    margin-bottom: 5px
}

.content.triggers .box .row {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    position: relative
}

.content.triggers .box .row label {
    text-transform: uppercase;
    font-size: 16px;
    margin: 0 10px 0 0;
    flex: none
}

.content.triggers .box .row .select_value {
    background: #f8f9fc;
    border-radius: 3px;
    padding: 10px 15px;
    margin: 5px 10px 5px 0;
    max-width: 460px;
    word-break: break-all
}

.content.triggers .box .row .html_frame {
    width: auto;
    margin: 0 10px 0 0
}

.content.triggers .box .row select {
    width: calc(100% - 4px) !important;
    border-radius: 3px;
    margin: 5px 0;
    height: 33px
}

.content.triggers .box .row select optgroup {
    font-style: normal;
    font-size: 13px;
    color: #46474f
}

.content.triggers .box .row input {
    width: calc(100% - 4px) !important;
    border-radius: 3px;
    margin: 5px 0;
    height: 33px
}

.content.triggers .box .icon {
    width: 18px;
    height: 18px;
    background: url(/images/sprite_icons_button.svg?v=2) no-repeat;
    background-size: 54px 900px;
    cursor: pointer
}

.content.triggers .box .icon.bin {
    background-position: 0 -450px;
    margin: 0 5px 0 auto;
    flex: none
}

.content.triggers .box .icon.bin:hover {
    background-position: -18px -450px
}

.content.triggers .box .icon.trigger {
    background-position: 0 -630px;
    margin: 0 4px 0 12px;
    cursor: default;
    flex: none
}

.content.triggers .box .button {
    margin: 0 0 0 auto;
    flex: none
}

.content.triggers .box .toggle {
    margin: 0 15px 0 5px;
    width: 35px;
    height: 10px;
    border-radius: 30px;
    background: #e2e5ef;
    position: relative;
    cursor: pointer;
    flex: none
}

.content.triggers .box .toggle .bullet {
    background: #cbced8;
    width: 18px;
    height: 18px;
    position: absolute;
    right: auto;
    left: 0;
    top: -4px;
    border-radius: 30px;
    transition: left .2s linear;
    cursor: pointer
}

.content.triggers .box .toggle .bullet.selected {
    background: #52cca5;
    left: calc(100% - 18px);
    right: auto
}

.content.triggers .box .toggle .bullet.red {
    background: #ff5b56
}

.content.triggers .col_1_4 p {
    line-height: 1.15
}

.content.triggers .col_1_4 table {
}

.content.triggers .col_1_4 table tr {
}

.content.triggers .col_1_4 table tr th {
    padding: 10px 0;
    border-color: #e2e5ef
}

.content.triggers .col_1_4 table tr td {
    padding: 10px 0;
    color: #6b6d7a;
    border-color: #e2e5ef
}

.content.triggers .col_1_4 table tr th:first-child, .content.triggers .col_1_4 table tr td:first-child {
    padding-right: 20px
}

.content.triggers .col_1_4 table tr td:nth-child(n) {
    background: 0 0
}

.unavailable {
    display: block;
    position: absolute;
    right: -10px;
    top: calc(100% + 10px);
    background: #fff;
    box-shadow: 0 5px 20px rgba(0, 0, 0, .1);
    z-index: 98;
    border-radius: 3px;
    text-align: center;
    padding: 30px;
    width: 300px;
    display: none
}

.unavailable .image.upgrade_plan {
    width: 100px;
    height: 100px;
    background: url(/images/sprite_empty.svg) no-repeat;
    background-size: 3000px 100px;
    background-position: -1900px 0;
    margin: 0 auto
}

.unavailable .title {
    font-size: 20px;
    font-weight: 700;
    text-align: center;
    margin: 0 0 20px
}

.unavailable p {
    font-size: 16px;
    text-align: center;
    line-height: 1.3
}

.unavailable p b {
    font-weight: 700;
    color: #46474f
}

.unavailable .button.blue {
    margin: 0 auto;
    padding: 6px 12px
}

.unavailable .button .icon.arrow_up {
    background-position: -18px -54px
}

@media (max-width: 1600px) {
    .content.triggers .box .row .html_frame {
        width: auto;
        min-width: 50px
    }
}

@media (max-width: 1040px) {
    .content.triggers .container .col_1_4 {
        order: 2;
        width: 100%;
        flex: 1 1 auto;
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
        margin-top: 20px
    }

    .content.triggers .container .col_3_4 {
        order: 1;
        width: 100%;
        flex: 1 1 auto
    }
}

@media (max-width: 860px) {
    .content.triggers .box {
        padding: 10px
    }

    .content.triggers .box .row {
        flex-wrap: wrap
    }

    .content.triggers .box .row label {
        width: 100px;
        padding-left: 5px
    }

    .content.triggers .box .row .html_frame {
        width: calc(100% - 110px);
        margin: 0
    }

    .content.triggers .box .row .select_value {
        width: calc(100% - 110px);
        margin-right: 0;
        font-size: 14px
    }

    .content.triggers .box .row .icon.trigger {
        display: none
    }

    .content.triggers .box .row .button.green {
        margin-top: 10px
    }

    .content.triggers .box .icon.bin {
        margin-top: 5px
    }

    .content.triggers .box .toggle {
        margin: 10px 5px 15px calc(100% - 40px)
    }
}