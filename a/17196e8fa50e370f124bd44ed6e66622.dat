.content.alerts .notification {
    background: #575e71;
    border-radius: 3px;
    padding: 15px 20px;
    color: #fff;
    font-weight: 300;
    text-align: left;
    line-height: 1.15;
    margin: 0 0 20px;
    display: flex;
    align-items: flex-start;
    animation: warning_pulse 2s ease-out infinite
}

.content.alerts .notification b {
    font-weight: 600;
    color: #fff
}

.content.alerts .notification p {
    font-weight: 300;
    color: rgba(255, 255, 255, .8);
    margin: 0
}

.content.alerts .notification a:link, .content.alerts .notification a:visited, .content.alerts .notification a:active {
    color: #fff;
    font-weight: 600;
    text-decoration: underline
}

.content.alerts .notification a:hover, .content.alerts .notification a:visited:hover {
    opacity: .6
}

.content.alerts .notification a.button.white:link, .content.alerts .notification a.button.white:visited, .content.alerts .notification a.button.white:active {
    color: #ff5b56;
    background: #fff;
    border: 2px solid #fff;
    font-weight: 800;
    text-decoration: none;
    display: inline-table;
    margin: 10px 10px 0 0;
    padding: 8px 18px
}

.content.alerts .notification a.button.white:hover, .content.alerts .notification a.button.white:visited:hover {
    opacity: .6
}

.content.alerts .notification a.button_outlined.white:link, .content.alerts .notification a.button_outlined.white:visited, .content.alerts .notification a.button_outlined.white:active {
    color: #fff;
    background: 0 0;
    border: 2px solid #fff;
    font-weight: 800;
    text-decoration: none;
    display: inline-table;
    padding: 8px 18px;
    border-radius: 5px;
    margin: 10px 0 0
}

.content.alerts .notification a.button_outlined.white:hover, .content.alerts .notification a.button_outlined.white:visited:hover {
    opacity: .6
}

.content.alerts .notification.red {
    background: #ff5b56
}

.content.alerts .notification.green {
    background: #52cca5
}

.content.alerts .notification.blue {
    background: #5767ff
}

.content.alerts .notification .icon {
    flex: none;
    width: 32px;
    height: 32px;
    background: url(/images/sprite_icons_big.svg?v=2) no-repeat;
    background-size: 96px 96px;
    margin: 0 20px 0 0;
    display: block
}

.content.alerts .notification .icon.warning {
    background-position: 0 -32px
}

.content.alerts .notification .text {
}

.page .content .align_center {
    width: 130px
}

.content.alerts .box_table table tr:nth-child(2n) td {
    background: 0 0
}

.content.alerts .box_table table tr th {
    position: relative
}

.content.alerts .box_table table tr th .tag_disabled {
    position: absolute;
    background: #46474f;
    color: #fff;
    padding: 8px 12px;
    border-radius: 3px 3px 0 0;
    top: -44px;
    width: 140px;
    text-align: center;
    left: 50%;
    margin-left: -70px
}

.content.alerts .box_table table tr td {
    position: relative
}

.content.alerts .box_table table tr td.temp_disabled {
    opacity: .3;
    position: relative
}

.content.alerts .box_table table tr td.temp_disabled:after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    cursor: default
}

.content.alerts .box_table table tr td .toggle {
    margin: 3px auto 0;
    width: 35px;
    height: 10px;
    border-radius: 30px;
    background: #e2e5ef;
    position: relative;
    cursor: pointer
}

.content.alerts .box_table table tr td .toggle .bullet {
    background: #cbced8;
    width: 18px;
    height: 18px;
    position: absolute;
    right: auto;
    left: 0;
    top: -4px;
    border-radius: 30px;
    transition: left .2s linear
}

.content.alerts .box_table table tr td .toggle .bullet.selected {
    background: #52cca5;
    left: calc(100% - 18px);
    right: auto
}

.content.alerts .box_table table tr td .unavailable {
    display: block;
    position: absolute;
    right: 100px;
    top: 60px;
    background: #fff;
    box-shadow: 0 5px 20px rgba(0, 0, 0, .1);
    z-index: 98;
    border-radius: 3px;
    text-align: center;
    padding: 30px;
    width: 300px;
    display: none
}

.content.alerts .box_table table tr td .unavailable .image.upgrade_plan {
    width: 100px;
    height: 100px;
    background: url(/images/sprite_empty.svg) no-repeat;
    background-size: 3000px 100px;
    background-position: -600px 0;
    margin: 0 auto
}

.content.alerts .box_table table tr td .unavailable .title {
    font-size: 20px;
    font-weight: 700;
    text-align: center;
    margin: 0 0 20px
}

.content.alerts .box_table table tr td .unavailable p {
    font-size: 16px;
    text-align: center;
    line-height: 1.3
}

.content.alerts .box_table table tr td .unavailable p b {
    font-weight: 700;
    color: #46474f
}

.content.alerts .box_table table tr td .unavailable .button.blue {
    margin: 0 auto;
    padding: 6px 12px
}

.content.alerts .box_table table tr td .unavailable .button .icon.arrow_up {
    background-position: -18px -54px
}

.content.alerts .box_table table tr td select {
    margin: 0 0 0 auto;

}

.content.alerts .box_table table tr .align-center {
    max-width: 200px
}

.content.alerts .box_table table tr td small {
    display: block;
    margin: 3px 0 0;
    font-size: 12px;
    color: #959cad;
    font-weight: 300
}

.content.alerts .box .row .row {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    margin: 5px 0 20px
}

.content.alerts .box .row:last-child .row {
    margin: 5px 0 0
}

.content.alerts .box .row .row .value {
    background: #f8f9fc;
    padding: 10px 12px;
    border-radius: 3px;
    color: #6b6d7a;
    font-size: 16px;
    font-weight: 300;
    margin: 0 auto 0 0;
    flex: 1 1 auto;
    min-height: 36px;
    word-break: break-all;
    overflow: hidden; white-space: nowrap; text-overflow: ellipsis;
}
.content.alerts .box .row .row .value:hover {
    cursor: pointer;
    background: rgba(255,255,255,.1);
}
.content.alerts .box .row .row .button {
    margin: 0 0 0 10px;
    font-size: 14px
}

.content.alerts .box .row .row .button:first-child {
    margin: 0
}

.content.alerts .box_table table tr th:last-child {
    width: 500px;
    padding-left: 40px
}

.content.alerts .box_table table tr td:last-child {
    width: 240px;
    padding-left: 40px
}

.content.alerts .empty_state .action_row .button.green {
    margin-right: auto
}

.content.alerts .empty_state .action_row .circle-loader {
    margin: 0 auto
}

.content.alerts .apps {
    display: flex;
    justify-content: space-between;
    margin: 0 -10px;
    flex-wrap: wrap
}

.content.alerts .apps .app_play {
    min-width: 135px;
    height: 40px;
    background: #000 url(/images/apps.svg) no-repeat top left;
    background-size: 135px 80px;
    text-indent: 102%;
    white-space: nowrap;
    overflow: hidden;
    display: block;
    margin: 0 10px 10px;
    box-shadow: 3px 3px 8px rgba(0, 0, 0, .3);
    transition: 100ms linear;
    flex: 1;
    border-radius: 3px
}

.content.alerts .apps .app_play:hover {
    opacity: .7;
    transform: scale(1.05)
}

.content.alerts .apps .app_store {
    min-width: 135px;
    height: 40px;
    background: #000 url(/images/apps.svg) no-repeat bottom left;
    background-size: 135px 80px;
    text-indent: 102%;
    white-space: nowrap;
    overflow: hidden;
    display: block;
    margin: 0 10px 10px;
    box-shadow: 3px 3px 8px rgba(0, 0, 0, .3);
    transition: 100ms linear;
    flex: 1;
    border-radius: 3px
}

.content.alerts .apps .app_store:hover {
    opacity: .7;
    transform: scale(1.05)
}

.content.alerts .action_row.row_sticky {
    position: sticky;
    bottom: 0;
    left: -10;
    right: -10;
    z-index: 5;
    padding-bottom: 20px
}

.content.alerts .action_row.row_sticky .floating_shadow {
    display: none;
    background: -moz-linear-gradient(top, rgba(255, 255, 255, 0) 0%, rgba(248, 249, 252, 1) 70%, rgba(248, 249, 252, 1) 100%);
    background: -webkit-linear-gradient(top, rgba(255, 255, 255, 0) 0%, rgba(248, 249, 252, 1) 70%, rgba(248, 249, 252, 1) 100%);
    background: linear-gradient(to bottom, rgba(255, 255, 255, 0) 0%, rgba(248, 249, 252, 1) 70%, rgba(248, 249, 252, 1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00ffffff', endColorstr='#f8f9fc', GradientType=0);
    height: 150px;
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: -1
}

@media (max-width: 1600px) {
    .content.alerts .box .row .row {
        flex-wrap: wrap
    }

    .content.alerts .box .row .row .value {
        width: 100%
    }

    .content.alerts .box .row .row .button {
        width: 100%;
        margin: 10px 0 0;
        justify-content: center
    }
}

@media (max-width: 1040px) {
    .content.alerts .container {
        flex-wrap: wrap
    }

    .content.alerts .container .col_3_4 {
        width: 100%
    }

    .content.alerts .container .col_1_4 {
        width: 100%
    }
}

@media (max-width: 860px) {
    .content.alerts .box_table table tr .rmv1 {
        display: none
    }

    .content.alerts .box_table table tr td {
        font-weight: 600
    }

    .content.alerts .box_table table tr .shw1 {
        display: block
    }

    .content.alerts .box_table table tr .shw1 .row {
        display: flex;
        align-items: space-between;
        margin: 20px 0 0;
        position: relative
    }

    .content.alerts .box_table table tr .shw1 .row label {
        font-weight: 300;
        font-size: 14px
    }

    .content.alerts .box_table table tr .shw1 .row .toggle {
        margin: 2px 0 0 auto
    }

    .content.alerts .box_table table tr .shw1 .row select {
        margin: -8px 0 0 auto;
        width: 210px;
        padding: 0 36px 0 5px;
        font-size: 14px
    }

    .content.alerts .box_table table tr .shw1 .row .unavailable {
        top: 30px;
        right: 0;
        width: 250px
    }

    .content.alerts .box_table table tr .shw1 .row .unavailable .button {
        padding: 8px 15px
    }

    .content.alerts .box_table table tr .shw1 .row .unavailable .title {
        font-size: 18px
    }

    .content.alerts .box_table table tr .shw1 .row .unavailable p {
        font-size: 14px
    }
}
