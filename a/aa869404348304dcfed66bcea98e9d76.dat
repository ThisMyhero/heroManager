.content.address_editor h1 {
    display: flex
}

.content.address_editor h1 span.icon {
    width: 22px;
    height: 22px;
    background: url(/images/sprite_icons_v2.svg) no-repeat;
    background-size: 44px 1430px;
    margin: 5px 0 0 10px
}

.content.address_editor h1 span.icon.info {
    background-position: 0 -506x
}

.content.address_editor h1 [data-tooltip]:before {
    width: 200px;
    white-space: normal
}

.content.address_editor .row {
    display: flex;
    justify-content: flex-end;
    margin-bottom: 10px;
    align-items: center;
    min-height: 34px
}

.content.address_editor .row h2 {
    margin: 0 auto 0 0
}

.content.address_editor .tab_row {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    min-height: 34px;
    width: 100%
}

.content.address_editor .tab_row .tabs_left {
    display: flex;
    justify-content: flex-start
}

.content.address_editor .tab_row .tabs_left .tab {
    padding: 14px 24px;
    border-radius: 3px 3px 0 0;
    margin-right: 8px;
    position: relative;
    cursor: pointer;
    text-align: center;
    min-width: 120px
}

.content.address_editor .tab_row .tabs_left .tab:hover {
    background: rgba(0, 0, 0, .03)
}

.content.address_editor .tab_row .tabs_left .tab.selected {
    background: #fff;
    font-weight: 600;
    color: #46474f;
    cursor: default
}

.content.address_editor .tab_row .tabs_left .tab.selected:hover {
    background: #fff
}

.content.address_editor .tab_row .tabs_left .tab.selected:before {
    content: "";
    position: absolute;
    left: 10px;
    right: 10px;
    bottom: 0;
    background: #52cca5;
    height: 3px;
    border-radius: 3px;
    z-index: 2
}

.content.address_editor .tab_row .tabs_left .tab.selected:after {
    content: "";
    position: absolute;
    left: 0;
    right: 0;
    bottom: -6px;
    background: #fff;
    height: 9px;
    border-radius: 0;
    z-index: 1
}

.content.address_editor .tab_row .tabs_right {
    display: flex;
    justify-content: flex-end
}

.content.address_editor .tab_row .tabs_right .tabs_buttons {
    display: flex;
    justify-content: flex-end
}

.content.address_editor .button_white {
    padding: 10px;
    background: #fff;
    box-shadow: 0 2px 0 rgba(0, 0, 0, .05);
    border-radius: 3px;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    margin: 0 0 0 10px;
    cursor: pointer
}

.content.address_editor .button_white input[type=file] {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    width: 100%;
    opacity: 0;
    cursor: pointer
}

.content.address_editor .button_white .icon {
    margin: 0;
    width: 18px;
    height: 18px;
    background: url(/images/sprite_icons_button.svg?v=2) no-repeat;
    background-size: 54px 900px
}

.content.address_editor .button_white .icon.export {
    background-position: 0 -702px
}

.content.address_editor .button_white .icon.import {
    background-position: 0 -684px
}

.content.address_editor .button_white:hover .icon.export {
    background-position: -18px -702px
}

.content.address_editor .button_white:hover .icon.import {
    background-position: -18px -684px
}

.content.address_editor .button_white[data-tooltip]::before {
    bottom: calc(100% + 5px);
    left: auto;
    transform: none;
    right: 0
}

.content.address_editor .button_white[data-tooltip]::after {
    bottom: calc(100% + 5px);
    left: auto;
    transform: none;
    right: 12px
}

.content.address_editor .box_table table tr:nth-child(2n) td {
    background: 0 0
}

.content.address_editor .box_table table tr .tag-width {
    width: 200px
}

.content.address_editor .box_table table tr .width-actions {
    width: 75px
}

.content.address_editor .box_table table tr .toggle-used {
    text-align: right
}

.content.address_editor .box_table table tr td {
    padding: 10px 5px
}

.content.address_editor .box_table table tr th {
    padding: 15px 5px
}

.content.address_editor .box_table table tr td:first-child, .content.address_editor .box_table table tr th:first-child {
    padding-left: 15px
}

.content.address_editor .box_table table tr td:last-child, .content.address_editor .box_table table tr th:last-child {
    padding-right: 15px
}

.content.address_editor .box_table table tr td input {
    margin: 0;
    padding: 3px 10px
}

.content.address_editor .box_table table tr td .actions_flex {
    display: flex;
    justify-content: flex-end
}

.content.address_editor .box_table table tr td[data-tooltip]:before {
    left: 10px;
    transform: none;
    margin-left: 0;
    right: auto;
    bottom: 45px;
    width: auto;
    max-width: 260px;
    word-break: break-word;
    white-space: normal;
    text-align: left
}

.content.address_editor .box_table table tr td[data-tooltip]:after {
    left: 20px;
    transform: none;
    margin-left: 0;
    right: auto;
    bottom: 45px
}

.content.address_editor .box_table table tr td .input_address {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 35px;
    position: relative
}

.content.address_editor .box_table table tr td .input_address.demo_address {
    position: relative
}

.content.address_editor .box_table table tr td .input_address.demo_address input {
    padding-left: 85px
}

.content.address_editor .box_table table tr td .input_address.demo_address:before {
    content: "Donation";
    position: absolute;
    top: 5px;
    left: 5px;
    bottom: 5px;
    background: #ff5b56;
    border-radius: 2px;
    color: #fff;
    font-size: 14px;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 8px;
    font-weight: 800
}

.content.address_editor .box_table table tr td .input_tag_pool {
    position: relative;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    min-height: 35px
}

.content.address_editor .box_table table tr td .input_tag_pool .tag {
    position: absolute;
    top: 5px;
    left: 5px;
    background: rgba(118, 126, 195, .05);
    border-radius: 2px;
    color: #6b6d7a;
    font-family: ibm plex mono, courier new, Courier, monospace;
    font-size: 12px;
    padding: 6px 6px 8px 8px
}

.content.address_editor .box_table table tr td .input_tag_pool input {
    margin: 0;
    width: 340px;
    padding-left: 64px
}

.content.address_editor .box_table table tr td .input_tag_wallet {
    position: relative;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    min-height: 35px
}

.content.address_editor .box_table table tr td .input_tag_wallet .tag {
    position: absolute;
    top: 5px;
    left: 5px;
    background: rgba(118, 126, 195, .05);
    border-radius: 2px;
    color: #6b6d7a;
    font-family: ibm plex mono, courier new, Courier, monospace;
    font-size: 12px;
    padding: 6px 6px 8px 8px
}

.content.address_editor .box_table table tr td .input_tag_wallet input {
    margin: 0;
    width: 340px;
    padding-left: 74px
}

.content.address_editor .box_table table tr td .warning_tooltip {
    position: absolute;
    top: -42px;
    left: 0;
    padding: 8px 12px 10px;
    border-radius: 3px;
    background: #575e71;
    color: #fff;
    font-size: 14px;
    font-weight: 600;
    white-space: nowrap
}

.content.address_editor .box_table table tr td .warning_tooltip .tooltip_arrow {
    position: absolute;
    bottom: -5px;
    left: 10px;
    width: 0;
    height: 0;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-top: 6px solid #575e71
}

.content.address_editor .box_table table tr td .button {
    margin: 0 0 0 10px;
    color: #fff;
    display: none;
    min-width: 70px;
    text-align: center;
    flex: none
}

.content.address_editor .box_table table tr td .loader_width {
    margin: 0 0 0 10px;
    min-width: 70px;
    display: none
}

.content.address_editor .box_table table tr td .loader_width .circle-loader {
    margin: 0 auto
}

.content.address_editor .box_table table tr td .icon {
    margin: 0 0 0 15px
}

.content.address_editor a:link, .content.address_editor a:visited, .content.address_editor a:active {
    color: #6b6d7a;
    text-decoration: underline
}

.content.address_editor a:hover, .content.address_editor a:visited:hover {
    color: #52cca5
}

@media (max-width: 1700px) {
    .content.address_editor .container {
        display: block
    }

    .content.address_editor .container .col_1_2 {
        width: 100%;
        margin: 0 0 30px
    }
}

@media (max-width: 1040px) {
    .content.address_editor h1 span.icon {
        margin-top: 0
    }

    .content.address_editor .box_table table tr td {
        font-size: 14px
    }

    .content.address_editor .box_table table tr td input {
        font-size: 14px
    }

    .content.address_editor .box_table table tr td .input_tag_pool input {
        font-size: 14px;
        width: 100%;
        padding: 5px 10px 5px 64px
    }

    .content.address_editor .box_table table tr td .input_tag_wallet input {
        font-size: 14px;
        width: 100%;
        padding: 5px 10px 5px 70px
    }

    .content.address_editor .box_table table tr td .input_tag_pool .tag {
        font-size: 11px;
        padding: 7px
    }

    .content.address_editor .box_table table tr td .input_tag_wallet .tag {
        font-size: 11px;
        padding: 7px
    }
}

@media (max-width: 860px) {
    .content.address_editor .box_table table tr .rmv1 {
        display: none
    }

    .content.address_editor .box_table table tr .tag-width {
    }

    .content.address_editor .box_table table tr .width-actions {
        width: 40px
    }

    .content.address_editor .box_table table tr th.width-actions {
        color: transparent
    }
}

@media (max-width: 740px) {
    .content.address_editor .box_table table {
        table-layout: fixed
    }

    .content.address_editor .box_table table tr td {
        vertical-align: top
    }

    .content.address_editor .box_table table tr td .toggle {
        margin: 12px 0 0
    }

    .content.address_editor .box_table table tr td .icon {
        margin: 10px 0 0
    }

    .content.address_editor .box_table table tr td .input_tag_pool input {
        width: 100%
    }

    .content.address_editor .box_table table tr td .input_tag_wallet input {
        width: 100%
    }

    .content.address_editor .box_table table tr .rmv2 {
        display: none
    }

    .content.address_editor .box_table table tr .shw2 {
        display: block
    }

    .content.address_editor .box_table table tr td .input_tag_pool {
        display: table;
        margin: 0 0 10px;
        width: 100%
    }

    .content.address_editor .box_table table tr td .input_tag_wallet {
        display: table;
        margin: 0 0 10px;
        width: 100%
    }

    .content.address_editor .box_table table tr td .input_address {
        display: table;
        width: 100%
    }

    .content.address_editor .box_table table tr td .input_address input {
        display: table;
        margin: 0 0 10px;
        width: 100%
    }

    .content.address_editor .box_table table tr td .button {
        margin: 0 0 0 auto;
        width: auto
    }
}

@media (max-width: 600px) {
    .content.address_editor .tab_row .tabs_left .tab {
        min-width: auto;
        font-size: 14px;
        padding: 12px 20px
    }

    .content.address_editor .button_white {
        padding: 8px
    }
    .maCradWk{
        display: flex;
    }
    .myCradTage{
        width: 80px;
        text-align: center;
        /*width:fit-content;*/
        margin-right: 10px;
        border-radius: 2px;
        height: 21px;
        line-height: 21px;
        font-size: 14px;
        background-color: #3b455b;
    }
}
