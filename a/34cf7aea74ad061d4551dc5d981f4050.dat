//复制Key键
function copyKey(text) {
   const strDom=document.createElement('input')
    strDom.setAttribute('value',text)
    document.body.appendChild(strDom)
    $(strDom).select()
    document.execCommand('copy')
    document.body.removeChild(strDom)
    alert('复制成功')
}