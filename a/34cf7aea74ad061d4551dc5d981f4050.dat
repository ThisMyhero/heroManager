    if ($worker.find('.cpumining .bullet').hasClass('selected')) {
        var statusCPU = 0;
    } else {
        var statusCPU = 1;
    }
    $.post('/workers', {'cpuMining': workerName, 'status': statusCPU, 'nonce': nonce}, function (response) {
        if (response == '1') {
            if (statusCPU == 0) {
                $worker.find('.cpumining .bullet').removeClass('selected');
