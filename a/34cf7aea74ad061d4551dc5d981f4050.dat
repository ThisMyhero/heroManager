                    $('#update_site .outlined_button, #update_site .red').removeClass('disabled');
                    clearTimeout(x)
                },2000)
            }else {
                $('#update_site .outlined_button, #update_site .red').removeClass('disabled');
                $('.circle-loader').hide();
                $('.message_err').show();
            }
        }
    })
}
