    padding-right: 20px;
    font-size: 14px;
    font-weight: 300;
    color: #6b6d7a;
    position: relative;
    display: flex;
    justify-content: flex-start;
    align-items: center
}

.popup#edit_task .text .task_area .col_1_4 .select_box .selected_option .arrow {
    width: 10px;
    height: 10px;
    background: url(/images/sprite_icons_button.svg?v=2) no-repeat;
    background-size: 54px 900px;
    margin: 0;
    position: absolute;
    top: 50%;
    margin-top: -3px;
    right: -1px;
    transform: rotate(90deg)
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu {
    position: absolute;
    top: 40px;
    left: -1px;
    right: -1px;
    background-color: #fff;
    box-shadow: 0 5px 35px rgba(0, 0, 60, .2);
    border-radius: 3px;
    z-index: 8;
    display: none
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu.display {
    display: block
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu .row {
    padding: 8px 15px;
    margin: 0;
    border-bottom: 1px solid #e2e5ef;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    position: relative;
    font-size: 14px;
    font-weight: 400;
    color: #6b6d7a
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu .row:hover {
    background: #f8f9fc
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu .row.selected {
    background: #f8f9fc
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu .row:first-child {
    border-radius: 3px 3px 0 0
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu .row:last-child {
    border-bottom: 0;
    border-radius: 0 0 3px 3px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .priority_bar {
    position: absolute;
    left: 2px;
    top: 2px;
    bottom: 2px;
    width: 3px;
    border-radius: 2px;
    background: #e2e5ef
}

.popup#edit_task .text .task_area .col_1_4 .select_box .priority_bar.high {
    background: #ff5b56
}

.popup#edit_task .text .task_area .col_1_4 .select_box .priority_bar.medium {
    background: #ff9970
}

.popup#edit_task .text .task_area .col_1_4 .select_box .priority_bar.low {
    background: #ffd66f
}

.popup#edit_task .text .task_area .col_1_4 .select_box .priority_bar.gray {
    background: #c3c7d6
}

.popup#edit_task .text .task_area .col_1_4 .select_box .priority_bar.green {
    background: #52cca5
}

.popup#edit_task .text .task_area .col_1_4 .select_box .status_bar {
    position: absolute;
    left: 2px;
    top: 2px;
    bottom: 2px;
    width: 3px;
    border-radius: 2px;
    background: #e2e5ef
}

.popup#edit_task .text .task_area .col_1_4 .select_box .status_bar.discarded {
    background: #ff5b56
}

.popup#edit_task .text .task_area .col_1_4 .select_box .avatar {
    width: 21px;
    height: 21px;
    margin-right: 5px;
    margin-left: -6px;
    border-radius: 3px;
    overflow: hidden;
    background: #e2e5ef
}

.popup#edit_task .text .task_area .col_1_4 .select_box .avatar img {
    width: 100%;
    height: 100%;
    object-fit: cover
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon {
    width: 22px;
    height: 22px;
    background: url(/images/sprite_icons_v4.svg?v=22) no-repeat;
    margin-right: 3px;
    margin-left: -6px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon.ongoing {
    background-position: -22px -1276px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon.completed {
    background-position: 0 -1298px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon.discarded {
    background-position: -22px -1298px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon.any {
    background-position: 0 -1320px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon.worker {
    background-position: -22px -1232px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon.group {
    background-position: -22px -1320px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .icon.global {
    background-position: 0 -1342px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu .row .avatar {
    margin-left: -6px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .select_menu .row .icon {
    margin-left: -8px
}

.popup#edit_task .text .task_area .col_1_4 .select_box .selected_option .priority_bar {
    left: -10px;
    top: -6px;
    bottom: -6px
}

.popup#edit_task .text .row select {
    background-color: #fff;
    width: 100%;
    border: 0;
    box-shadow: 0 2px 0 rgba(0, 0, 0, .05)
}

@media (max-width: 960px) {
    .popup#edit_task {
        width: calc(100% - 40px);
        max-width: unset;
        left: 20px;
        right: 20px;
        top: 20px;
        transform: none;
        position: absolute
    }

    .popup#edit_task.overflow_screen {
        width: calc(100% - 40px);
        max-width: unset;
        left: 20px;
        right: 20px;
        top: 20px;
        transform: none;
        position: absolute
    }
}

@media (max-width: 700px) {
    .popup#edit_task .text {
        text-align: left
    }

    .popup#edit_task .text .task_area {
        display: block
    }

    .popup#edit_task .text .task_area .col_3_4 {
        width: 100%
    }

    .popup#edit_task .text .task_area .col_1_4 {
        width: 100%
    }

    .popup#edit_task .text textarea {
        height: 160px
    }

    .popup#edit_task .text .task_area .col_3_4 .task_title {
        margin-bottom: 10px
    }

    .popup#edit_task .text .task_area .col_3_4 .task_description_frame .frame {
        padding-left: 26px
    }

    .popup#edit_task .text .task_area .col_3_4 .task_description_frame {
        margin-bottom: 20px
    }
}

.bigImg {
    width: 100%;
    position: absolute;
    top: 50%;
    left: 50%;
    /*图片向左移动自身宽度的50%, 向上移动自身高度的50%。*/
    transform: translate(-50%,-50%);
}
/*遮罩层*/
.opacityBottom {
    width: 100%;
    height: 100%;
    position: fixed;
    background: rgba(0,0,0,0.8);
    z-index: 1000;
    top: 0;
    left: 0;
}
